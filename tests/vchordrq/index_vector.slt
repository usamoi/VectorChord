CREATE TABLE t (index serial primary key, val vector(64));

statement ok
INSERT INTO t (val)
SELECT
    l2_normalize(ARRAY(
        SELECT
            ('x' || substring(md5((64 * i + j)::text), 1, 16))::bit(64)::bigint / 18446744073709551615.0
        FROM generate_series(1, 64) d(j)
    )::vector)
FROM generate_series(1, 2048) s(i);

