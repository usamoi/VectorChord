CREATE TABLE t (val0 vector(3));

statement ok
INSERT INTO t (val0) 
SELECT 
    ARRAY[random(), random(), random()]::real[]::vector
FROM generate_series(1, 10000);

statement ok
CREATE INDEX ind0 ON t USING vchordrq (val0 vector_l2_ops);

statement ok
SET enable_seqscan TO off;

